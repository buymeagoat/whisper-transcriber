# Example environment variables - copy to .env.local for local development
VITE_API_URL=http://localhost:8001
VITE_APP_NAME=Whisper Transcriber
VITE_APP_VERSION=1.0.0
VITE_DEBUG=true

# Feature flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_TOOLS=true

# Upload configuration
VITE_MAX_FILE_SIZE=104857600
VITE_ALLOWED_FILE_TYPES=audio/wav,audio/mp3,audio/m4a,audio/flac

# Copy this file to .env.local and customize for your local development
