{
  "name": "whisper-transcriber-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "dev:clean": "rm -rf node_modules/.vite && npm run dev",
    "analyze": "vite build --mode analyze",
    "migrate": "node ../scripts/settings-migration-cli.js",
    "migrate:status": "node ../scripts/settings-migration-cli.js status",
    "migrate:backup": "node ../scripts/settings-migration-cli.js backup",
    "migrate:restore": "node ../scripts/settings-migration-cli.js restore",
    "migrate:validate": "node ../scripts/settings-migration-cli.js validate",
    "migrate:clean": "node ../scripts/settings-migration-cli.js clean",
    "migrate:init": "node ../scripts/settings-migration-cli.js init"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "axios": "^1.5.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.0.0",
    "lucide-react": "^0.287.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.3.5",
    "terser": "^5.44.0",
    "vite": "^4.4.5",
    "vite-bundle-analyzer": "^1.2.3"
  }
}
