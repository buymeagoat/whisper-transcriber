{
  "dashboard": {
    "id": null,
    "title": "Whisper Transcriber - Performance Analytics",
    "tags": ["whisper-transcriber", "performance"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Throughput",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"whisper-app\"}[5m]))",
            "legendFormat": "Total Requests/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"whisper-app\",status=~\"2..\"}[5m]))",
            "legendFormat": "Successful Requests/sec",
            "refId": "B"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"whisper-app\",status=~\"4..\"}[5m]))",
            "legendFormat": "Client Errors/sec",
            "refId": "C"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"whisper-app\",status=~\"5..\"}[5m]))",
            "legendFormat": "Server Errors/sec",
            "refId": "D"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "API Endpoint Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"whisper-app\"}[5m])) by (le, endpoint))",
            "legendFormat": "{{endpoint}} 95th percentile",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 3,
        "title": "Transcription Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(transcription_duration_seconds_bucket[5m])) by (le, model))",
            "legendFormat": "{{model}} 50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(transcription_duration_seconds_bucket[5m])) by (le, model))",
            "legendFormat": "{{model}} 95th percentile",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 4,
        "title": "Database Query Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket[5m])) by (le, operation))",
            "legendFormat": "{{operation}} 95th percentile",
            "refId": "A"
          },
          {
            "expr": "rate(db_queries_total[5m])",
            "legendFormat": "Queries/sec",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m])",
            "legendFormat": "Cache Hits/sec",
            "refId": "A"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "legendFormat": "Cache Misses/sec",
            "refId": "B"
          },
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "File Processing Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(file_upload_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "Upload Time 95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(file_processing_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "Processing Time 95th percentile",
            "refId": "B"
          },
          {
            "expr": "rate(file_uploads_total[5m])",
            "legendFormat": "Files Uploaded/sec",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Memory Usage by Component",
        "type": "graph",
        "targets": [
          {
            "expr": "container_memory_usage_bytes{name=~\"whisper-.*\"} / 1024 / 1024 / 1024",
            "legendFormat": "{{name}} Memory GB",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (GB)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 8,
        "title": "CPU Usage by Component",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\"whisper-.*\"}[5m]) * 100",
            "legendFormat": "{{name}} CPU %",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "CPU Usage %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 9,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_network_receive_bytes_total{name=~\"whisper-.*\"}[5m]) / 1024 / 1024",
            "legendFormat": "{{name}} RX MB/s",
            "refId": "A"
          },
          {
            "expr": "rate(container_network_transmit_bytes_total{name=~\"whisper-.*\"}[5m]) / 1024 / 1024",
            "legendFormat": "{{name}} TX MB/s",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "MB/s",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40}
      },
      {
        "id": 10,
        "title": "Disk I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_fs_reads_bytes_total{name=~\"whisper-.*\"}[5m]) / 1024 / 1024",
            "legendFormat": "{{name}} Read MB/s",
            "refId": "A"
          },
          {
            "expr": "rate(container_fs_writes_bytes_total{name=~\"whisper-.*\"}[5m]) / 1024 / 1024",
            "legendFormat": "{{name}} Write MB/s",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "MB/s",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40}
      },
      {
        "id": 11,
        "title": "Queue Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "celery_queue_length",
            "legendFormat": "{{queue}} Queue Length",
            "refId": "A"
          },
          {
            "expr": "rate(celery_tasks_total[5m])",
            "legendFormat": "Tasks/sec",
            "refId": "B"
          },
          {
            "expr": "celery_workers_active",
            "legendFormat": "Active Workers",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 48}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "time_range",
          "type": "interval",
          "query": "1m,5m,15m,30m,1h,6h,12h,1d",
          "current": "5m"
        },
        {
          "name": "model",
          "type": "query",
          "query": "label_values(transcription_duration_seconds, model)",
          "refresh": 1,
          "includeAll": true
        }
      ]
    }
  }
}